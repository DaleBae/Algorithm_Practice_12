#include <stdio.h>#include <string.h>
int main(void){ char str[100]={"0"}; char alpha[30]={"0"}; int arr[30]={0}; int i=0,j=0,a=0;
 for(i=0;i<=25;i++){  alpha[i]=i+97; } scanf("%s",str);
 for(i=0;i<=25;i++){  for(j=0;j<strlen(str);j++)   if(alpha[i]==str[j])    arr[i]+=1; }
 for(i=0;i<=25;i++){  if(arr[i]>=1)   a++; }  if(a%2==0)  printf("CHAT WITH HER!"); else  printf("IGNORE HIM!");
}}
