import java.util.Scanner;

public class Seven263A {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int m[][] = new int[4][4];
		
		for(int i=0; i<5; i++) {
			for(int j=0; j<5; j++) {
				if(sc.nextInt()==1) {
					System.out.println(Math.abs(2-i)+Math.abs(2-j));
				}
			}
		}	
	}
	
}
