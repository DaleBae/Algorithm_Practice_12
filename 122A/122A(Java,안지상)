import java.util.Scanner;

public class one122A {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		String n = sc.next();
		int v = sc.nextInt();
		

		if(n.matches(".*[1235689].*")) 	{
			if(v%4==0 || v%7==0 || v%47==0) 
				System.out.println("YES");
			else
				System.out.println("NO");
			
		} else {
			System.out.println("YES");
		}
		
		
		
	}
}
