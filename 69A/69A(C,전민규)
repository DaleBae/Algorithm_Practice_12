#include <stdio.h>
int main(void){ int n=0; int num[200][200]={0}; int i=0,j=0,c=0; int a[3]={0};
 scanf("%d",&n);
 for(i=0;i<n;i++){  for(j=0;j<3;j++){   scanf("%d",&num[i][j]);   a[j]+=num[i][j];  } }
 for(i=0;i<3;i++){  if(a[i]==0)   c++; }
 if(c==3)  printf("YES"); else  printf("NO");
 return 0;}
