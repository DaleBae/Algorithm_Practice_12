#include<iostream>

using namespace std;

int main(void) {
	int n;
	cin >> n;
	int a = 0, b = 0, c = 0;
	for (int i = 0; i < n; i++) {
		int temp1, temp2, temp3;
		cin >> temp1 >> temp2 >> temp3;
		a += temp1;
		b += temp2;
		c += temp3;
	}
	if (a == 0 && b == 0 && c == 0) {
		cout << "YES";
	}
	else {
		cout << "NO";
	}
}
